import java.util.*;

public class ListImplementation {

	//	List Interface Implementation

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.println("\n================== Basic list Implementation =====================\n");

		List <Integer> list1 = List.of(1, 2, 3, 4, 5, 6, 7);
		System.out.println(list1);
		
		//		List : ArrayList, LinkedList, Vector, Stack
		
		System.out.println("\n================== Array list Implementation =====================\n");
		
		ArrayList <Integer> list2 = new ArrayList<Integer>();
		int i = 0;
		while(i < 10) {
			list2.add(i);
			i++;
		}
		System.out.print("ArrayList Generated By While Loop: ");
		System.out.println(list2);

		System.out.println("\n================== Linked list Implementation =====================\n");
		
		LinkedList <Integer> list3 = new LinkedList<Integer>();
		int j = 0;
		while(j < 10) {
			list3.add(j);
			j++;
		}
		System.out.print("LinkedList Generated By While Loop: ");
		System.out.println(list3);
		
		System.out.println("\n================== Vector Implementation =====================\n");
		
		Vector <Integer> list4 = new Vector<Integer>();
		int k = 0;
		while(k < 10) {
			list4.add(k);
			k++;
		}
		System.out.print("Vector Generated By While Loop: ");
		System.out.println(list4);
		
		System.out.println("\n================== Vector Implementation =====================\n");
		
		Stack <Integer> list5 = new Stack<Integer>();
		k = 0;
		while(k < 10) {
			list5.push(k); // Stack Follows LIFO Implementation
			k++;
		}
		System.out.print("Vector Generated By While Loop: ");
		System.out.println(list5);
				
		
	}
	
}
