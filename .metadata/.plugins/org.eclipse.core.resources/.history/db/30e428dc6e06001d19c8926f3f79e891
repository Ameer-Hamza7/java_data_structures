import java.util.*;

public class SetImplementation {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("\n================== HashSet Implementation =====================\n");

		HashSet <Integer> set1 = new HashSet<Integer>();
		int k = 0;
		while(k < 10) {
			set1.add(k); // Set is an unordered collection of non-duplicate items
			k++;
		}
		set1.add(1);
		set1.add(1);
		set1.add(2);
		set1.add(2);
		System.out.print("HashSet Generated By While Loop: ");
		System.out.println(set1);

		System.out.println("\n================== LinkedHashSet Implementation =====================\n");

		LinkedHashSet <Integer> set2 = new LinkedHashSet<Integer>();
		k = 0;
		while(k < 10) {
			set2.add(k); // Set is an unordered collection of non-duplicate items
			k++;
		}

		System.out.print("LinkedHashSet Generated By While Loop: ");
		System.out.println(set2);


		System.out.println("\n================== SortedSet / TreeSet Implementation =====================\n");

		SortedSet <Integer> set3 = new TreeSet<Integer>();
		k = 20;
		while(k > 0) {
			set3.add(k); // Sorted Set organize non-duplicate value in ordered form ( By Default Ascending Order )
			k++;
		}

		System.out.print("SortedSet Generated By While Loop: ");
		System.out.println(set3);
	}

}
