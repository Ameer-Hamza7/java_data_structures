import java.util.*;

public class QueueImplementation {

	public QueueImplementation() {
		// TODO Auto-generated constructor stub

		System.out.println("\n================== Queue Implementation =====================\n");

		PriorityQueue <Integer> list5 = new PriorityQueue<Integer>();
		int k = 0;
		while(k < 10) {
			list5.add(k); // Queue Follows FIFO Implementation so we use 
			k++;
		}
		System.out.print("Stack Generated By While Loop: ");
		System.out.println(list5);
				
	}

}
